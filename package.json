{
  "name": "can",
  "version": "1.0.0",
  "description": "react + webpack 项目脚手架",
  "scripts": {
    "analyze": "npm run clean && digo copyStatic && cross-env NODE_ENV=development DEFINED_ENV=dev webpack --progress --config scripts/webpack/webpack.analyze.js --color",
    "build:dev": "npm run clean && digo copyStatic && cross-env NODE_ENV=development DEFINED_ENV=dev webpack --progress --config scripts/webpack/webpack.dev.js --color",
    "build:prd": "npm run clean && digo copyStatic && cross-env NODE_ENV=production DEFINED_ENV=prd webpack --progress --config scripts/webpack/webpack.prd.js --color",
    "clean": "digo clean dist",
    "start": "npm run clean && digo copyStatic && cross-env NODE_ENV=development DEFINED_ENV=dev webpack-dev-server --progress --config scripts/webpack/webpack.dev.js"
  },
